{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/sendEmail\\.json\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/arrepentimiento\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/ayuda\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/calculadora\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/contacto\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/favicon\\.ico\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/manifest\\.json\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/nosotros\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/robots\\.txt\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/solicitud\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}